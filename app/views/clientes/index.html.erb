<p style="color: green"><%= notice %></p>

<h1>Clientes - Quantidade de clientes <%= @quantidade_de_cientes %></h1>

<div id="clientes">
  <% @clientes.each do |cliente| %>
    <%= render cliente %>
    <p>
      <%= link_to "Show this cliente", cliente %>
    </p>
  <% end %>
</div>

<%= link_to "New cliente", new_cliente_path, class: "btn btn-primary" %>
